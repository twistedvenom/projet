{% extends "baseAdmin.html.twig" %}
{% block container %}
    <div class="col-lg-12">
        <h1 class="page-header">Professional Forms</h1>
    </div>
    {{ form_start(form) }}

    <div class="form-group">
        <label class="col-form-label" for="inputSuccess">Last Name </label>
        {{ form_widget(form.proname,{'attr': {'class': 'form-control'}} )}}
    </div>
    <div class="form-group">
        <label class="col-form-label" for="inputSuccess"> First Name</label>
        {{ form_widget(form.prosurname,{'attr': {'class': 'form-control'}} )}}
    </div>
    <div class="form-group">
        <label class="col-form-label" for="inputSuccess"> Email</label>
        {{ form_widget(form.email,{'attr': {'class': 'form-control'}} )}}
    </div>

    <div class="form-group">
        <label class="col-form-label" for="inputSuccess"> Softskills</label>
        {{ form_widget(form.softskills,{'attr': {'class': 'form-control'}} )}}
    </div>
    <div class="form-group">
        <label class="col-form-label" for="inputSuccess"> CV</label>
        {{ form_widget(form.image,{'attr': {'class': 'form-control'}} )}}
    </div>

    {{ form_rest(form) }}

{% endblock %}